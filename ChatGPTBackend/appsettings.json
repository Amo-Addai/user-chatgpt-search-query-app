{
  "https_port": 443,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseType": "SqlServer",
  "ConnectionStrings": {
    "Default": "Data Source=/Data/sqlite.db;",
    "Sqlite": "Data Source=/Data/sqlite.db;",
    "SqlServer": "Server={SQL-SERVER-ADDRESS};Database={DEVELOPMENT-DB-NAME};User={USERNAME};Password={PASSWORD};",
    "MySql": "Server={SQL-SERVER-ADDRESS};Database={DEVELOPMENT-DB-NAME};Uid={USERNAME};Pwd={PASSWORD};"
  },
  "JwtSettings": {
    "Secret": "your_secret_key_here"
  }
}
